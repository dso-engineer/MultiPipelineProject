            steps {
                echo 'Building on feature-1...'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing on feature-1...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying on feature-1...'
            }
        }
    }
}
